{
  "name": "webpack-rollup-parcel",
  "version": "1.0.0",
  "parcel": "./dist/parcel.js",
  "parcel-ts": "./dist/parcel-ts.js",
  "parcel-react": "./dist/parcel-react.js",
  "parcel-react-ts": "./dist/parcel-react-ts.js",
  "targets": {
    "parcel": {
      "source": "./src/index.js"
    },
    "parcel-ts": {
      "source": "./src/index.ts"
    },
    "parcel-react": {
      "source": "./src/index.jsx"
    },
    "parcel-react-ts": {
      "source": "./src/index.tsx"
    }
  },
  "scripts": {
    "webpack": "webpack",
    "webpack-ts": "webpack --config ./webpack.config.ts.js",
    "webpack-react": "webpack --config ./webpack.config.react.js",
    "webpack-react-ts": "webpack --config ./webpack.config.react-ts.js",
    "rollup": "rollup --config --bundleConfigAsCjs",
    "rollup-ts": "rollup --config ./rollup.config.ts.js --bundleConfigAsCjs",
    "rollup-react": "rollup --config ./rollup.config.react.js --bundleConfigAsCjs",
    "rollup-react-ts": "rollup --config ./rollup.config.react-ts.js --bundleConfigAsCjs",
    "parcel": "parcel build --no-source-maps"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^9.1.3",
    "parcel": "^2.9.3",
    "rollup": "^3.26.2",
    "ts-loader": "^9.4.4",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
